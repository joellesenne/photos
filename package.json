{
  "name": "photos",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "My gallery photos",
  "bugs": {
    "url": "https://github.com/joellesenne/photos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joellesenne/photos.git"
  },
  "license": "MIT",
  "author": "Joel Lesenne <contact@joellesenne.dev>",
  "scripts": {
    "build": "env-cmd -f .env.production react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "serve": "serve -s build -p 3080",
    "start": "env-cmd -f .env.development react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.7.9",
    "env-cmd": "^10.1.0",
    "exif-js": "^2.3.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0"
  }
}
