  "scripts": {
    "build": "env-cmd -f .env react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint - fix src/**/*.{js,jsx,}",
